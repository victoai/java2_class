package java2prj1.lamda;

public class Runnable학생 {

	public static void main(String[] args) {
		Runnable khs = () -> {
			for(int i=2; i<=9; i++) {
				for(int j=1; j<=9; j++) {
					System.out.println(i + "X" + j + " = " + i*j);
				}
				System.out.println("");
			}
		};
		
		
	  khs.run();
		

	  
		Runnable kjy = () -> {

			// 실행하고 싶은 코드
			for (int i = 1; i <= 9; i++) {
				for (int j = 1; j <= 9; j++) {
					System.out.println(i * j);
				}
			}
		};
		kjy.run();
	}

}
